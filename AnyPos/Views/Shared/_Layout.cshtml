<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <title>@ViewData["Title"] - AnyPos</title>

    <environment include="Development">

        <!-- Animate.css -->
        <link rel="stylesheet" href="~/Content/css/animate.css">
        <!-- Icomoon Icon Fonts-->
        <link rel="stylesheet" href="~/Content/css/icomoon.css">
        <!-- Bootstrap  -->
        <link rel="stylesheet" href="~/Content/css/bootstrap.css">
        <!-- Magnific Popup -->
        <link rel="stylesheet" href="~/Content/css/magnific-popup.css">
        <!-- Flexslider  -->
        <link rel="stylesheet" href="~/Content/css/flexslider.css">
        <!-- Owl Carousel -->
        <link rel="stylesheet" href="~/Content/css/owl.carousel.min.css">
        <link rel="stylesheet" href="~/Content/css/owl.theme.default.min.css">

        <!-- Date Picker -->
        <link rel="stylesheet" href="~/Content/css/bootstrap-datepicker.css">
        <!-- Flaticons  -->
        <link rel="stylesheet" href="~/Content/fonts/flaticon/font/flaticon.css">
        <!-- Theme style  -->
        <link rel="stylesheet" href="~/Content/css/style.css">
        <!-- Modernizr JS -->
        <script src="~/Content/js/modernizr-2.6.2.min.js"></script>
    </environment>
    <environment exclude="Development">
        <link rel="stylesheet" href="https://stackpath.bootstrapcdn.com/bootstrap/3.4.1/css/bootstrap.min.css"
              asp-fallback-href="~/lib/bootstrap/dist/css/bootstrap.min.css"
              asp-fallback-test-class="sr-only" asp-fallback-test-property="position" asp-fallback-test-value="absolute" />
        <link rel="stylesheet" href="~/css/site.min.css" asp-append-version="true" />

    </environment>
</head>
